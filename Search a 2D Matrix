class Solution {
public:
    bool searchMatrix(vector<vector<int>>& matrix, int x) {
        int n = matrix[0].size();
        for(int i=0;i<matrix.size();i++){
            if(matrix[i][n-1]>=x){
                for(int j=0;j<matrix[i].size();j++){
                    if(matrix[i][j]==x){
                        return true;
                    }
                }
            }
        }
        
        return false;
        
    }
};
